<Window x:Class="trackr.ImageFeedWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:trackr"
        mc:Ignorable="d"
        Title="ImageFeedWindow" Height="450" Width="800">
    <Grid>
        <Image HorizontalAlignment="Stretch" Name="frameHolder"  VerticalAlignment="Stretch"  Stretch="Fill"/>
        <Button x:Name="configCameraButton" Content="Button" HorizontalAlignment="Left" Margin="10,378,0,0" VerticalAlignment="Top" Width="75" Click="configCameraButton_Click"/>
    </Grid>
</Window>
